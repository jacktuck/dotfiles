# toggle full screen
hyper - f : yabai -m window --toggle zoom-fullscreen
hyper - x : yabai -m window --toggle float;yabai -m window --grid 7:7:1:1:5:5

# set focus
# focus the prev window in a stack
hyper - p : yabai -m window --focus stack.prev
# focus the next window in a stack
hyper - n : yabai -m window --focus stack.next
# focus the most recently focused window in a stack
hyper - a : yabai -m window --focus stack.recent

# switch apps
meh - t : open -a "Alacritty"
meh - g : open -a "Google Chrome"
meh - c : open -a "Slack"
meh - f : open -a "Finder"
meh - s : open -a "Spotify"
meh - n : open -a "Notes"
meh - w : open -a "Whatsapp"
